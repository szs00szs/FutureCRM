<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.future.hist.crm.dao.DepartmentMapper">

	<select id="getAllDepartment" resultType="Department">
		select *from department d where d.id is not null
	</select>
	<insert id="insert" parameterType="Department">
		insert into department(remark, name,principal,incumbent)
		values(#{remark},#{name},#{principal}, #{incumbent})
	</insert>
</mapper>